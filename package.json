{
  "name": "cpceed-student-app",
  "version": "0.1.0",
  "private": true,
  "description": "Client for time logging app for the CPCEED program at Texas Tech University",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TTUSDC/cpceed-student-app.git"
  },
  "author": "TTU SDC",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TTUSDC/CPCEEDWebApp/issues"
  },
  "homepage": "https://github.com/TTUSDC/CPCEEDWebApp#readme",
  "dependencies": {
    "@material-ui/core": "1.4.3",
    "@material-ui/icons": "2.0.1",
    "axios": "0.16.1",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "firebase": "5.3.1",
    "grommet": "^1.11.0",
    "immutability-helper": "2.7.1",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-hot-loader": "4.3.4",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "redux": "4.0.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "lint": "node_modules/.bin/eslint src test",
    "docs": "node_modules/.bin/esdoc"
  },
  "devDependencies": {
    "@firebase/app-types": "0.3.2",
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "23.4.2",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chai": "4.1.2",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "esdoc": "0.5.2",
    "esdoc-node": "1.0.2",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "17.0.0",
    "eslint-loader": "2.1.0",
    "eslint-plugin-chai-friendly": "0.4.1",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.10.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "flow-bin": "^0.78.0",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "23.4.2",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react-dev-utils": "^5.0.1",
    "resolve": "1.6.0",
    "sinon": "6.1.4",
    "sinon-chai": "3.2.0",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./src/setupTest.js",
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "globals": {
      "ENV": "test"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleDirectories": [
      "<rootDir>node_modules",
      "<rootDir>src",
      "<rootDir>__test__",
      "<rootDir>common"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  }
}
